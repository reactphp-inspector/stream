{
    "name": "react-inspector/stream",
    "description": "\ud83c\udf0a React Stream throughput inspector",
    "license": "MIT",
    "authors": [
        {
            "name": "Cees-Jan Kiewiet",
            "email": "ceesjank@gmail.com"
        }
    ],
    "require": {
        "php": "^7.4",
        "nikic/php-parser": "^4.3",
        "react-inspector/metric": "^1.0",
        "react-inspector/metrics": "^1.0",
        "react/event-loop": "^1.0",
        "react/stream": "^1.0",
        "reactivex/rxphp": "^2.0",
        "roave/better-reflection": "^4.0",
        "wyrihaximus/constants": "^1.5",
        "wyrihaximus/iterator-or-array-to-array": "^1.1"
    },
    "require-dev": {
        "wyrihaximus/async-test-utilities": "^8.0"
    },
    "config": {
        "platform": {
            "php": "7.4"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "ReactInspector\\Stream\\": "src/"
        },
        "files": [
            "src/bootstrap.php",
            "src/include.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "ReactInspector\\Tests\\Stream\\": "tests/"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "composer normalize"
        ],
        "post-update-cmd": [
            "composer normalize"
        ]
    }
}
